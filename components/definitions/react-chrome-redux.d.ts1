declare module 'react-chrome-redux' {
  interface Action {
    type: any
  }

  interface AnyAction extends Action {
    [extraProps: string]: any
  }

  interface Unsubscribe {
    (): void
  }

  type Reducer<S> = (state: S, action: AnyAction) => S
}
